#=================================================
# spur / ypspur-coordinatorの起動( spur-joystickはecho )
#=================================================
  [[spur]]
    [[[child0]]]
      position = 0:0
      type = Window
      order = 0
      parent = ""
      size = 1918, 1053
    [[[child1]]]
      position = 959
      type = HPaned
      order = 0
      parent = child0
    [[[terminal2]]]
      profile = default
      type = Terminal
      order = 0
      parent = child1 
      title = ypspur-coordinator
      command = ypspur-coordinator -p ~/rpp/Loop_new.param -d /dev/ttyACM0 --high-resolution; bash; 
    [[[terminal3]]]
      profile = default
      type = Terminal
      order = 1
      parent = child1
      title = ( ypspur-joystick )
      command = echo "ypspur-joystick -V 0.5 -W 0.5 -A 0.5 -O 0.7 -d /dev/input/js0 0.5 0.8"; bash; 


