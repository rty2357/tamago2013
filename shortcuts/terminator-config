[global_config]
[keybindings]
[profiles]
  [[default]]
[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
    [[[window0]]]
      type = Window
      parent = ""

#=================================================
# urg / urg-proxyとls-coordinate-converterの起動
#=================================================
  [[urg]]
    [[[child0]]]
      position = 0:0
      type = Window
      order = 0
      parent = ""
      size = 1918, 1053
    [[[child1]]]
      position = 959
      type = HPaned
      order = 0
      parent = child0
    [[[child2]]]
      position = 526
      type = VPaned
      order = 0
      parent = child1
    [[[child5]]]
      position = 526
      type = VPaned
      order = 1
      parent = child1
    [[[terminal3]]]
      profile = default
      type = Terminal
      order = 0
      parent = child2
      title = urg-proxy( /dev/ttyACM0 )
      command = cd ${tamago_home}/urg-proxy; ./launcher -p /dev/ttyACM0; bash; 
    [[[terminal4]]]
      profile = default
      type = Terminal
      order = 1
      parent = child2
      title = ls-coordinate-converter( front )
      command = sleep 1; cd ${tamago_home}/ls-coordinate-converter; ./launcher -g front-ls-coordinate-converter.conf; bash; 
    [[[terminal6]]]
      profile = default
      type = Terminal
      order = 0
      parent = child5
      title = urg-proxy( /dev/ttyACM1 )
      command = cd ${tamago_home}/urg-proxy; ./launcher -p /dev/ttyACM1; bash;  
    [[[terminal7]]]
      profile = default
      type = Terminal
      order = 1
      parent = child5
      title = ls-coordinate-converter( upward )
      command = sleep 1; cd ${tamago_home}/ls-coordinate-converter; ./launcher -g upward-ls-coordinate-converter.conf; bash; 

